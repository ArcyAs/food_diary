@model List<FoodDiary.Repositories.Entities.UserDetailsEntity>
@{
    ViewData["Title"] = "Personal Details";
}

<h2>Personal Details</h2>
<table class="table table-hover">
    <tr>
        <th>Gender</th>
        <th>Height</th>
        <th>Bmi</th>
        <th>Bmr</th>
        <th>Weight</th>
        <th>AddData</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
             <td>@item.Gender</td>
             <td>@item.Height</td>
             <td>@item.Bmi</td>
             <td>@item.Bmr</td>
             <td>@item.Weight</td>
             <td>@item.AddDate</td>
        </tr>
    }
</table>
